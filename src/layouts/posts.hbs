<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Matt&amp;Meg</title>
    <link rel="stylesheet" href="../github-markdown.css">
  </head>
  <body class="markdown-body">
    {{> header }}
    <h1 class="title">{{title}}</h1>
    <time class="date" pubdate="{{date}}">{{date}}</time>
    {{#markdown}}
      {{> body }}
    {{/markdown}}
    {{> footer }}
  </body>
</html>